{
    "name": "limesoda-com/ls-security-headers",
    "description": "Configures security headers like content security policy",
    "type": "typo3-cms-extension",
    "homepage": "https://www.limesoda.com",
    "license": ["GPL-2.0-or-later"],
    "keywords": [
        "TYPO3 CMS",
        "extension",
        "LIMESODA",
        "Security Headers"
    ],
    "require": {
        "typo3/cms-core": "^11.5 || ^12",
        "php": ">=7.4 <=8.3.99"
    },
    "autoload": {
        "psr-4": {
            "LimeSoda\\LsSecurityHeaders\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ls_security_headers"
        }
    }
}
